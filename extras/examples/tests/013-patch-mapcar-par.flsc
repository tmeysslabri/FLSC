;;;; itération, forme standard
;;;; sur une gamme à 8 tons égaux
((patch (num) (* 0.25 (range 1 (+ 3 num)))
   (apply par
     (flop
       (mapcar
         (lambda (i)
           [[i (+ 3 i)]
            (m* (subsign [1] (menv [0 0.1 0]))
                (mosc (* 200 (** 2 (/ i 8))) 0))])
         (range 0 (- num 1)))))) 17)

