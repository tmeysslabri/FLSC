;;;; exemple de nowarp (distribution de signal)
((patch (freq) [1 2 3 4]
   (nowarp ((freq (mseg freq (* 4 freq))))
     (let ((sound (lambda (pitch)
                    (m* (menv [0 0.1 0]) (mosc (m* pitch freq))))))
       (par [[0 2] [1 3] [2 4]]
         [(sound 1) (sound 0.75) (sound 0.5)])))) 220)

